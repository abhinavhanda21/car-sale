<script>is_amp=null;function otp_editLink(){jQuery('#otp_updateMobile').show();jQuery('#otp_verifyMobileShowMobileText').hide();$(".edit-link").hide();jQuery('#miss_call_err').hide();$('#otpDiv').addClass('otp-fade');$(".otp-update").val('');$(".otp-update").focus();$("#otpMain").hide();$("#otpHead").html('Please enter the new mobile number');$("#otp_code1").val('');enableSubmitBtn();}function changeMobile(){var mobile=jQuery('.otp-update').val();if(mobile==""){jQuery("#changedMobileNo_err").show();jQuery('#updateMobile_err').text("Please enter your mobile number.").show();jQuery('.otp-update').focus();return false;}else if(!/^\s*[9876][0-9]{9}\s*$/.test(mobile)){jQuery("#changedMobileNo_err").show();jQuery('#updateMobile_err').text("Please enter valid mobile number.").show();jQuery('.otp-update').focus();return false;}var formMobileInputId=jQuery('#otp_formMobileInputId').val();jQuery('#'+formMobileInputId).val(jQuery('.otp-update').val());jQuery('#otp_mobileNoToBeVerified').val(mobile);jQuery('#otp_verifyMobileShowMobileText').html(mobile);var flag='change no';jQuery('#otp_updateMobile').hide();jQuery('#otp_verifyMobileShowMobileText').show();$(".edit-link").show();$("#otpMain").show();$("#otpHead").html('Please enter the 4 digit code sent to your mobile number.');verifyMobileNo(flag);return false;}function verifyMobileNo(flag){jQuery('#otp-resend-link').hide();var callbackFunction=jQuery('#otp_callbackFunction').val();var mobile=jQuery('#otp_mobileNoToBeVerified').val();jQuery('#otp_verifyMobileShowMobileText').html(mobile);jQuery('#otp_verifyMobileChangeBox').val('');jQuery('#changedMobileNo_err').hide();var checkEveryTimeForm=jQuery('#otp_callbackFunction').val();jQuery.post(TODBFILEPATH+'/verifymobile.php',{"MOBILE":mobile},function(mobileVerificationStatus){jQuery("#bgPopup").val("");if(checkEveryTimeForm=='registerUserFromSellCar'){mobileVerificationStatus=0;jQuery("#bgPopup").val("sellCarProcess");}if(mobileVerificationStatus==1){jQuery('#otp_verifyMobileStatus').val(1);jQuery("#otp_mobileVerificationStatus").val(1);window[callbackFunction]();jQuery('#otp').modal('hide');if($(window).width()<768){$('.closePopup').addClass('gorp-m-cross-pos');}else{$('.closePopup').addClass('gorp-d-cross-pos');}return false;}else{callga('mobile-popup-err','On Road Price');if(flag=='change no'){generateOTP();jQuery('#otp_code1').focus();startOtpCounter('otp_counter','otp-link');setTimeout(function(){jQuery('#otp-resend-link').show();$('.otp_counter').hide();},30000);}else{jQuery('#verifyMobileError').text("We haven't received a missed call yet.").slideDown();}jQuery("#otp_verifyMobileShowMobileText").html(mobile);}setTimeout(function(){jQuery('#otp_code1').focus();},300);});}function generateOTP(type){jQuery('#verifyMobileError').hide();jQuery('#OTPloadingImg').show();jQuery('#generateOTPButton').hide();$("#otp_code1").val('');jQuery('#otp_code_err').hide();$('#otpMain').show();$('#otp_verifyMobileShowMobileText').show();$('.edit-link').show();$("#otpHead").html('Please enter the 4 digit code sent to your mobile number.');enableSubmitBtn();var mobile=jQuery('#otp_mobileNoToBeVerified').val();var userName=jQuery('#otp_userToBeVerified').val()
if(userName==undefined||userName==''){userName='';}var flag='generateOtp';jQuery.post(TODBFILEPATH+'/get_verified_otp_2db.php',{"mobile":mobile,"flag":flag,"userName":userName},function(data){if(data!=""){jQuery('#OTP,#verifyOTPButton').show();jQuery('#generateOTPButton').show();jQuery('#MVloadingImg,#OTPloadingImg').hide();if(type=='resend'){jQuery('#otp-resend-link').hide();jQuery('#otp_code1').focus();$("#otp_code1").val('');startOtpCounter('otp_counter','otp-resend-link');setTimeout(function(){jQuery('#otp-resend-link').show();$('.otp_counter').hide();},30000);}}});}function verifyOTP(type){var mobile=jQuery('#otp_mobileNoToBeVerified').val();var otp_code=jQuery('#otp_code1').val();var callbackFunction=jQuery('#otp_callbackFunction').val();var otp_len=otp_code.length;jQuery('#otp_verifyMobileShowMobileText').html(mobile);if(type!='misscall'){if(otp_code==''){jQuery('#otp_code_err').text("Please enter OTP code.").show();jQuery('#otp_code1').focus();jQuery('#verifyOTPButton').show();jQuery('#generateOTPButton').show();jQuery('#OTPloadingImg').hide();return false;}if(otp_len<4){jQuery('#otp_code_err').text("Please enter valid OTP code.").show();jQuery('#otp_code1').focus();jQuery('#verifyOTPButton').show();jQuery('#generateOTPButton').show();jQuery('#OTPloadingImg').hide();return false;}}jQuery("#changedMobileNo_err").hide();jQuery('#miss_call_err').hide();var flag='verifyOtp';$("#otpSubmitDiv").hide();$("#OTPloadingImg").show();jQuery.post(TODBFILEPATH+'/get_verified_otp_2db.php',{"mobile":mobile,"flag":flag,'otp_code':otp_code,'type':type},function(data){if(data!=""){console.log('--'+data+'--');if(data=='Verified'){callGTMLayerEvent("Verified",GTMLayer_MakeModelName,source,'');jQuery('#otp_verifyMobileStatus').val(1);jQuery("#otp_mobileVerificationStatus").val(1);jQuery("#otp_financeVerificationStatus").val(1);window[callbackFunction]();jQuery('#otp').modal('hide');$("#otpSubmitDiv").show();$("#OTPloadingImg").hide();ga('send','event',eventCategory,'click','otp-close');return false;}else{$("#otpSubmitDiv").show();$("#OTPloadingImg").hide();if(type=='misscall'){jQuery('#miss_call_err').show();jQuery('#OTPloadingImg').hide();jQuery('#verifyOTPButton').show();jQuery('#generateOTPButton').show();return false;}else{jQuery('#otp_code_err').text("Enter a valid OTP.").show();jQuery('#otp_code1').focus();jQuery('#OTPloadingImg').hide();jQuery('#verifyOTPButton').show();jQuery('#generateOTPButton').show();return false;}}}else{return false;}});}function otpClosebtn(){var callbackFunction=jQuery('#otp_callbackFunction').val();jQuery('#otp_mobileVerificationStatus').val(0);var showPopup=jQuery("#bgPopup").val();if(exception_flag!=undefined&&exception_flag=='webForm'){$('.modal-backdrop').css('display','none');}src_name='';if(showPopup==''){src_name='orp';jQuery('#GORP').modal({show:true});window[callbackFunction]();jQuery('#otp').modal('hide');}else if(showPopup=='dealerOffer'){src_name='dcb';jQuery('#dealerOffer').modal({show:true});jQuery('#otp').modal('hide');window[callbackFunction]();}else if(showPopup=='alertLaunch'){src_name='upcoming';jQuery('#alertLaunch').modal({show:true});jQuery('#otp').modal('hide');window[callbackFunction]();}else if(showPopup=='bike_finance'){jQuery('#bike_finance').modal({show:true});jQuery('#otp').modal('hide');window[callbackFunction]();}else if(showPopup=='requestcallback'){jQuery('#callbackPopup').modal({show:true});jQuery('#otp').modal('hide');window[callbackFunction]();}else{jQuery('#otp').modal('hide');jQuery('#'+showPopup).show();}if(showPopup!='bike_finance'&&showPopup!='requestcallback'){if(is_amp!=undefined){dataLayer.push({"event":"popup","virtual_action":src_name+"_otp_closed_amp"});}else{dataLayer.push({"event":"popup","virtual_action":src_name+"_otp_closed"});}}ga('send','event',eventCategory,'click','otp-close');return false;}function enableSubmitBtn(){if(jQuery('#otp_code1').val().length>3){$('#otpSubmitDiv').removeClass('ty-noneActive').removeClass('otp-fade');$('#otpDiv').removeClass('otp-fade');}else{$('#otpSubmitDiv').addClass('ty-noneActive').addClass('otp-fade');$('#otpDiv').addClass('otp-fade');}}function verifyMobileViaAPI(){var mobile=jQuery('#otp_mobileNoToBeVerified').val();}</script><!--googleoff: all-->
