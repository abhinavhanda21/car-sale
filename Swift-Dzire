<script type="text/javascript" src="https://www.zigwheels.com/forum/js/login_md5.js?v=519" async></script>
<script src="https://apis.google.com/js/platform.js?onload=onLoadGoogleCallback" async defer></script>
<script>function statusChangeCallback(response){if(response.status==='connected'){fbLogin(response.authResponse.accessToken);}}function checkLoginState(){FB.getLoginStatus(function(response){statusChangeCallback(response);});}window.fbAsyncInit=function(){FB.init({appId:'140130286031515',cookie:true,xfbml:true,version:'v2.5'});};function fb_login(){FB.login(function(response){if(response.authResponse){checkLoginState();access_token=response.authResponse.accessToken;user_id=response.authResponse.userID;FB.api('/me',function(response){user_email=response.email;});}else{console.log('User cancelled login or did not fully authorize.');}},{scope:'public_profile,email,user_likes'});}(function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(d.getElementById(id))return;js=d.createElement(s);js.id=id;js.src="//connect.facebook.net/en_US/sdk.js";fjs.parentNode.insertBefore(js,fjs);}(document,'script','facebook-jssdk'));function fbLogin(accessToken){FB.api('/me',{locale:'en_US',fields:'name, email, id'},function(response){var name=response.name.replace(/ /gi,'_');var image='';var email=response.email;var fbuserid=response.id;var pswd='Alfa123';$('#idLoginBtn').css('display','block');$('#idLoginBtn').append('<img src="https://images.zigcdn.com/images/loading.gif" >');$.post('https://www.zigwheels.com/2db/forum_google_login_api.php?token='+accessToken+'&email='+email+'&name='+name+'&image='+image+'&source=fb&fbuserid='+fbuserid,{},function(data1){var result=data1.split('@@');var pageUrl=result[1];var ask_the_community=getParameterByNameLogin('ask_the_community');var vehicle_type=getParameterByNameLogin('vehicle_type');if(ask_the_community==1&&vehicle_type=='4Wheeler'){localStorage.removeItem("ask_the_community");localStorage.removeItem("vehicle_type");pageUrl="aHR0cHM6Ly93d3cuemlnd2hlZWxzLmNvbS9mb3J1bS9uZXctY29udGVudC83MzI=";}else if(ask_the_community==1&&vehicle_type=='2Wheeler'){localStorage.removeItem("ask_the_community");localStorage.removeItem("vehicle_type");pageUrl="aHR0cHM6Ly93d3cuemlnd2hlZWxzLmNvbS9mb3J1bS9uZXctY29udGVudC83MzM=";}$('#current_page_url').val(pageUrl);$('#provider').val('facebook');$('#fb_web').val('fb_web');$('#auth').val(accessToken);var action='https://www.zigwheels.com/forum/auth/loginExternal';$("#idLoginForm").attr('action',action);$('#idLoginForm').submit();});});}function onLoadGoogleCallback(){gapi.load('auth2',function(){auth2=gapi.auth2.init({client_id:'591154493254.apps.googleusercontent.com',cookiepolicy:'single_host_origin',scope:'profile'});auth2.attachClickHandler(element,{},function(googleUser){var name=googleUser.getBasicProfile().getName().replace(/ /gi,'_');;var image=googleUser.getBasicProfile().getImageUrl();var email=googleUser.getBasicProfile().getEmail();var accessToken=googleUser.getBasicProfile().getId();var pswd='Alfa123';$('#idLoginBtn').css('display','block');$('#idLoginBtn').append('<img src="https://images.zigcdn.com/images/loading.gif" >');$.post('https://www.zigwheels.com/2db/forum_google_login_api.php?token='+accessToken+'&email='+email+'&name='+name+'&image='+image+'&source=google&fbuserid=0',{},function(data1){var result=data1.split('@@');var pageUrl=result[1];var ask_the_community=getParameterByNameLogin('ask_the_community');var vehicle_type=getParameterByNameLogin('vehicle_type');if(ask_the_community==1&&vehicle_type=='4Wheeler'){localStorage.removeItem("ask_the_community");localStorage.removeItem("vehicle_type");pageUrl="aHR0cHM6Ly93d3cuemlnd2hlZWxzLmNvbS9mb3J1bS9uZXctY29udGVudC83MzI=";}else if(ask_the_community==1&&vehicle_type=='2Wheeler'){localStorage.removeItem("ask_the_community");localStorage.removeItem("vehicle_type");pageUrl="aHR0cHM6Ly93d3cuemlnd2hlZWxzLmNvbS9mb3J1bS9uZXctY29udGVudC83MzM=";}$('#current_page_url').val(pageUrl);if(result.length==3){$('#provider').val('google');$('#fb_web').val('fb_web');$('#auth').val(result[2]);var action='https://www.zigwheels.com/forum/auth/loginExternal';$("#idLoginForm").attr('action',action);}else{$('#idLoginUserName').val(result[0]);$('#idLoginPassword').val(pswd);}$('#idLoginForm').submit();});},function(error){console.log('Sign-in error',error.error);});});element=document.getElementById('googleSignIn');}function getParameterByNameLogin(name,url){if(!url)url=window.location.href;name=name.replace(/[\[\]]/g,"\\$&");var regex=new RegExp("[?&]"+name+"(=([^&#]*)|&|#|$)"),results=regex.exec(url);if(!results)return null;if(!results[2])return'';return decodeURIComponent(results[2].replace(/\+/g," "));}function show_old_login(){$("#loginBoxUserName").focus();$(".fo-nw-step1").hide();$(".fo-nw-step2").show();$(".fo-nw-step3").hide();$(".fo-nw-step4").hide();}function login_back_button(toShow,toHide){$('.'+toShow).show();$('.'+toHide).hide();}function vb_login(pageUrl){$('#current_page_url').val(pageUrl);$('#idLoginUserName').val($('#loginBoxUserName').val());$('#idLoginPassword').val($('#loginBoxPassword').val());if($('#loginBoxUserName').val()!=''&&$('#loginBoxPassword').val()!=''){$("#error-msg-login").html('');$.ajax({url:TODBFILEPATH+"/forum_login_api.php",type:"GET",async:false,data:{uname:encodeURI($('#loginBoxUserName').val()),'password':$('#loginBoxPassword').val()},success:function(response){$('#idLoginForm').submit();}});}else{$("#error-msg-login").html('Please enter User Name and password');return false;}}function edit_username(){$('#idUpdateUserName').attr('readonly',false).focus();$('.fo-nw-edit').hide();$('#idUserUpdateUsernameBtn').attr('disabled',false);}function user_update_username(){var userName=$('#idUpdateUserName').val();if(userName==""){$("#error-msg-update").html('Please enter User Name');return false;}else if(userName==localStorage.getItem("forum_header_userName")||userName==""){$('#myModal3').modal('hide');return true;}else if(userName.indexOf(" ")>=0){$("#error-msg-update").html('Space is not allowed in User Name');return false;}else if(/^[a-zA-Z0-9_@-]*$/.test(userName)==false){$("#error-msg-update").html('Special characters are not allowed in User Name');return false;}else{var email="";if(email==''){email=$('#idUpdateEmail').val();}$('#idUserUpdateUsernameBtn').attr('disabled',true);$.post('https://www.zigwheels.com/2db/forum_user_update_username_api.php?user_name='+userName+'&email='+email+'',{},function(data){if($.trim(data)=='successful'){$("#error-msg-update").removeClass('error-msg');$("#error-msg-update").css('color','green');$("#error-msg-update").html('User Name Updated Sucessfully !');setTimeout(function(){$('#myModal3').modal('hide');},100);}else{$('#idUserUpdateUsernameBtn').attr('disabled',false);$("#error-msg-update").html('User Name alredy taken, Try a new one! ');}});}}</script>
