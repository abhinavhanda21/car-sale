'498948');Moengage.add_user_attribute("maxPrice",'828947');if(localStorage.getItem("forum_header_userId")===null||localStorage.getItem("forum_header_userId")===undefined){Moengage.add_user_attribute("forumUserLoginStatus",'No');}else{Moengage.add_user_attribute("forumUserLoginStatus",'Yes');Moengage.add_user_attribute("forumUserName",localStorage.getItem("forum_header_userName"));Moengage.add_user_attribute("forumUserId",localStorage.getItem("forum_header_userId"));if(localStorage.getItem("forum_header_vehicleList")===null||localStorage.getItem("forum_header_vehicleList")===undefined){Moengage.add_user_attribute("forumVehiclesAdded",'No');}else{Moengage.add_user_attribute("forumVehiclesAdded",'Yes');Moengage.add_user_attribute("forumUserGarage",localStorage.getItem("forum_header_vehicleList"));}}moe_track('car-model-page','car','landing-page','Maruti Suzuki','Swift');if(sessionStorage.moeEvModelId!='1368'){sessionStorage.moeEvModelId='1368';Moengage.track_event("car-model-viewed",{'make':'Maruti Suzuki','model':'Swift','make_url':'Maruti-Suzuki','model_url':'Swift','modelpage_url':"https://www.zigwheels.com/newcars/Maruti-Suzuki/Swift",'model_image':'https://media.zigcdn.com/media/model/2018/Jan/maruti-swift-right_600x300.jpg','city':'Ludhiana','city_url':'ludhiana','page_type':'car-model'});}}catch(e){}}
function moe_track(event_name,key,key_data,make,model,variant){if(key!==null&&typeof key==='object'){Moengage.track_event(event_name,key);}else{key_data=key_data.replace(" ","-");if(make==undefined){var arg={key:key,"value":key_data};}else{var arg={key:key,"value":key_data,"make":make,"model":model,"variant":variant,"make_url":"Maruti-Suzuki","model_url":"Swift","model_image":"https://media.zigcdn.com/media/model/2018/Jan/maruti-swift-right_420x210.jpg","forumModelCountOfExpert":"3","forumModelCountOfOwner":"113"};}Moengage.track_event(event_name,arg);}}function moenMsgShowHide(d){var moe_cdb=document.getElementById('chrome_mobile_backend');if(d==1){document.getElementById('moe_hd').innerHTML=((screen.width<=768)?'Exclusively For You!':'Just For You!');document.getElementById('moe_txt').innerHTML=((screen.width<=768)?'Be the first to know about latest offers on Swift in your city. Press Allow':'Be the first to know about latest offers on Swift in your city. Click Allow');if(isFirefox()){var className_moe='FireFox';}else if(isOpera()){var className_moe='Opera';}else{var className_moe='chrome';}moe_cdb.className=className_moe;moe_cdb.style.display='block';}else{moe_cdb.style.display='none';}}function isChrome(){var vendor=(navigator&&navigator.vendor||'').toLowerCase();var match=/google inc/.test(vendor)?userAgent.match(/(?:chrome|crios)\/(\d+)/):null;return match!==null&&!isOpera();}function isOpera(){var userAgent=(navigator&&navigator.userAgent||'').toLowerCase();var match=userAgent.match(/(?:^opera.+?version|opr)\/(\d+)/);return match!==null;}function isFirefox(){var userAgent=(navigator&&navigator.userAgent||'').toLowerCase();var match=userAgent.match(/(?:firefox|fxios)\/(\d+)/);return match!==null;}function getSetMoenData(lsName,n){if(n==undefined||n=='undefined'&&n==''){return;}n=n.replace(/( ,)+/g,",").replace(/(, )+/g,",").trim();var moeData=localStorage.getItem('moengageZWData');if(moeData!=null){moeData=JSON.parse(moeData);}else{moeData=new Object();}var moeT=parseInt(new Date().getDate()-new Date(moeData['time']).getDate());if(!isNaN(moeT)&&moeT>30){moeData=new Object();}var d=moeData[lsName];var nd='';if(d!=undefined&&d!='undefined'&&d!=''&&n!=undefined&&n!='undefined'&&n!=''){nd=d+','+n;}else if(n!=undefined&&n!='undefined'&&n!=''){nd=n;}if(nd!=''){nd=nd.replace(/( ,)+/g,",").replace(/(, )+/g,",").trim();moeData['time']=new Date();var rd=Array.from(new Set(nd.split(",").reverse())).reverse().slice(-10).join(",");moeData[lsName]=rd;}localStorage.setItem('moengageZWData',JSON.stringify(moeData));return rd;}</script>
